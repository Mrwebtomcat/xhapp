.tabmain{
    width: 100%;
    height: calc(100% - 474px);
    .sdbox{
        top:425px;
    }
    .imgtips{
        width: 664px;
        margin:50px auto 20px;
        img{
            width: 100%;
        }
    }
    .tabvie-header{
        width: 694px;
        background: #fff;
        margin: auto;
        box-shadow: 0px 10px 5px #333;
        height: 100px;
        display: flex;
        align-items: center;
        position: relative;
        z-index: 1000;
        .moneyja_bar{
            width: 100%;
            height: 100%;
            top: 0;
        }
        a{
            width: 33.3%;
            height: 100%;
            line-height: 100px;
            font-size: 34px;
            text-overflow: initial;
            &.mui-control-item{
                
                &.mui-active{
                    color: #FDB913;
                }
            } 
        }
        ul{
            width: 100%;
            height: 100px;
            display: flex;
            justify-content: space-between;
            padding: 0 74px;
            width: 100%;
            align-items: center;
        }
        li{
            width: 100px;
            &.ative{
                color: #ff8700;
            }    
        }
    }
    .mui-slider-group{
        top:120px;
    }
    .tabvie-con{
        width: 100%;
        height: 100%;
        overflow-y: scroll;
        padding-bottom:50px; 
        &::-webkit-scrollbar{
            display: none;
        }
        .mui-table-view{
            text-align: left;
           
            &:before{
                background: transparent;
            }
        }
         .mui-table-view-cell:active{
             background: transparent;
         }
        .mui-table-view-cell{
            padding: 28px 28px 20px;
            height: 120px;
           @include posr;
            &:after{
                 background: #d1d1d1;
            }
            .celltop{
                font-size: 32px;
                color: #333;
            }
            .cellbottom{
                margin-top: 18px;
                font-size: 24px;
                color: #a6a6a6;
            }
            .cellshownum{
                @include posa;
                font-size: 32px;
                height: 32px;
                top: 0px;
                bottom: 0;
                margin: auto;
                right: 28px;
                &.a9cf5{
                    color: #1a9cf5;
                }
                &.fdb913{
                    color: #fdb913;
                }
            }
        }
    }
}


